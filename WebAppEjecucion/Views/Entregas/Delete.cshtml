@model WebAppEjecucion.Models.Entregas

@{
    ViewBag.Title = "Delete";
}

<h2>Eliminar Entrega</h2>

<h3>¡Seguro que desea eliminar los datos de la entrega?</h3>
<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Obra
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Obra.Obra1)
        </dd>

        <dt>
            Fecha de Entrega
        </dt>

        <dd>
            @Html.DisplayFor(model => model.FechaEntrega)
        </dd>

        <dt>
            Cantidad Entregada
        </dt>

        <dd>
            @Html.DisplayFor(model => model.CantEntregada)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Eliminar" class="btn btn-default" /> |
            @Html.ActionLink("Atras", "Index", "Entregas", new { id = Model.IdObra }, new { @class = "btn btn-danger" })
        </div>
    }
</div>
